# Copy this file to .env.local (preferred) or .env and fill in values.
# .env.local is loaded before .env.

# -----------------------------------------------------------------------------
# GitHub access (optional for public repos)
# -----------------------------------------------------------------------------
GITHUB_TOKEN=

# -----------------------------------------------------------------------------
# Runtime quality/profile defaults
# -----------------------------------------------------------------------------
# local | s3 | hybrid
DEVPORT_SNAPSHOT_BACKEND=hybrid

# Positive integer bytes (optional lease-cache cap for hydration)
DEVPORT_LEASE_CACHE_MAX_BYTES=

# v1 | v2
DEVPORT_PLANNER_VERSION=v2

# standard | strict
DEVPORT_QUALITY_GATE_LEVEL=strict

# -----------------------------------------------------------------------------
# S3 snapshot backend (required when DEVPORT_SNAPSHOT_BACKEND=s3)
# -----------------------------------------------------------------------------
DEVPORT_S3_BUCKET=
DEVPORT_S3_REGION=
DEVPORT_S3_PREFIX=snapshots

# AWS credentials for the SDK default credential chain.
# Use either AWS_PROFILE, or key/secret (and optional session token).
AWS_PROFILE=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=

# -----------------------------------------------------------------------------
# OpenAI + database (required for persist / persist-section / finalize)
# -----------------------------------------------------------------------------
OPENAI_API_KEY=

DEVPORT_DB_HOST=localhost
DEVPORT_DB_PORT=5432
DEVPORT_DB_NAME=db
DEVPORT_DB_USER=user
DEVPORT_DB_PASSWORD=password
